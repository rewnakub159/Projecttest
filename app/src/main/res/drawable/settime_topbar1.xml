<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="@android:color/black" />
            <solid android:color="#fff" />
            <stroke
                android:width="5dp"
                android:color="#C1F3EB"

                android:dashWidth="500dp" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <corners
                android:topRightRadius="600dp"
                android:bottomRightRadius="600dp"
                />
        </shape>
    </item>

</layer-list>